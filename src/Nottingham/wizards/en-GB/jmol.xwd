<wizard menus="Media">  <pageWizard remove="true" duplicate="false">    <newNodes>      <jmol><![CDATA[<jmol name="Enter Page Title" text="Enter text for the page" xyz="Select an XYZ file" size="Medium" initLook="Ball and Stick" colour="Default" controls="true"/>]]></jmol>    </newNodes>  </pageWizard>  <!-- JMOL VIewer================================= -->  <jmol menu="Media" menuItem="JMOL Viewer" hint="Provides a viewer for JMOL files" icon="icAtom" thumb="thumbs/jmol.jpg" remove="true" deprecated="We cannot redistribute GPL components. You must install JMOL manually. See instructions in the JMOL folder">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Text" type="TextArea" height="150"/>		<model type="group" label="Model">		<xyz label="File" type="Media"/>		<size type="ComboBox" options="Small,Medium,Large" label="Size" defaultData="Medium"  width="100"/>	</model>		<init type="group" label="Initial Appearance">		<initLook label="Model Type" options="CPK,Ball and Stick,Sticks,Wireframe,Cartoon,Backbone" type="ComboBox" data="CPK,Ball and Stick,Sticks,Wireframe,Cartoon,Backbone" defaultData="Ball and Stick"  width="120"/>		<colour type="ComboBox" options="Default,Element,Amino Acid,Chain,Secondary Structure" label="Colour" defaultValue="Default" width="140"/>	</init>	    <controls label="Show Default Controls" type="CheckBox" defaultValue="true"/>    <cartoonOpt label="Cartoon Option" type="CheckBox" defaultValue="true" optional="true"/>    <backOpt label="Backbone Option" type="CheckBox" defaultValue="true" optional="true"/>    <colourOpt type="CheckBox" label="Control Bar - Colour" optional="true"/>    <labelOpt type="CheckBox" label="Control Bar - Label" optional="true"/>    <surfaceOpt type="CheckBox" label="Control Bar - Surface" optional="true"/>    <scriptOpt type="CheckBox" label="Control Bar - Script" optional="true"/>    <initScript type="TextInput" label="Initial Jmol Script" optional="true"/>    <bgColour label="Background Colour" type="ComboBox" options="White,Black,Blue" width="120" optional="true"/>    <initSpin label="Spin" type="CheckBox" optional="true"/>    <xyz2 label="2nd Model File" type="Media" optional="true"/>    	<newNodes>      <jmolOption><![CDATA[<jmolOption name="Enter option label here" script="Enter Jmol script here"/>]]></jmolOption>    </newNodes>  </jmol>    <jmolOption menuItem="Script Option" icon="icBullet" remove="true">  	<name label="Label" type="TextInput" wysiwyg="true"/>	<script label="Jmol Script" type="TextArea" height="100"/>    </jmolOption></wizard>