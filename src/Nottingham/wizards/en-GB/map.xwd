<wizard menus="Misc">  <pageWizard remove="true" duplicate="false">    <newNodes>      <map><![CDATA[<map name="Enter Page Title" text="Enter text for the page here" align="left" mapurl="Paste the link here" maptype="roadmap" tooltip="Enter a Description for Accessibility" link="true"/>]]></map>    </newNodes>  </pageWizard>  <!-- GOOGLE MAP============================ -->  <map icon="icMap" menu="Misc" menuItem="Google Map" hint="Use the map to include an interactive map in the presentation. You can add optional layers of KML data to the map." thumb="thumbs/googleMap.jpg" remove="true" deprecated="This page is no longer supported">    <name label="Page Title" type="TextInput" wysiwyg="true"/>    <text label="Instructions" type="textArea" height="300"/>    <align label="Align Text" options="Left,Right" type="ComboBox" data="left,right" defaultValue="left" width="100"/>    <mapurl label="Map Link" type="textInput"/>    <maptype label="Map Type" options="Roadmap,Satellite,Terrain,Hybrid" type="ComboBox" data="roadmap,satellite,terrain,hybrid" defaultValue="roadmap" width="100"/>    <tooltip label="Map Description" type="TextInput"/>    <link label="Link to Map" type="checkbox" defaultValue="true"/>  </map></wizard>